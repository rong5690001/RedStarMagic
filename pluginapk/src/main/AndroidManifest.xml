<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redstar.magic.pluginapk">

    <application
        android:name=".PluginApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChajianActivity" />
        <activity
            android:name=".TestFragmentActivity"
            android:process=":remote" />
        <activity android:name=".DatabindingActivity" />
        <activity android:name=".PluginLifeCycleActivity" />
        <activity
            android:name=".SingleTaskActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ActivityTestActivity" />

        <activity
            android:name=".SingleTopActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".SingleInstanceActivity"
            android:launchMode="singleInstance" />
    </application>

</manifest>